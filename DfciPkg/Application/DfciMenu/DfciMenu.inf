[Defines]
  INF_VERSION                    = 0x00010005
  BASE_NAME                      = DfciMenu
  FILE_GUID                      = ae8f0389-6ba2-47d3-9e55-80b3dd9b8a98
  MODULE_TYPE                    = DXE_DRIVER
  VERSION_STRING                 = 1.0
  ENTRY_POINT                    = DfciMenuEntry

#
# The following information is for reference only and not required by the build tools.
#
#  VALID_ARCHITECTURES           = IA32 X64 IPF EBC
#

[Sources]
  DfciPrivate.h
  DfciMenu.h
  DfciMenu.c
  DfciMenuVfr.Vfr
  DfciMenuStrings.uni
  DfciRequest.h
  DfciRequest.c
  DfciUsb.h
  DfciUsb.c
  DfciUpdate.h
  DfciUpdate.c

[Packages]
  DfciPkg/DfciPkg.dec
  NetworkPkg/NetworkPkg.dec
  MdePkg/MdePkg.dec
  MdeModulePkg/MdeModulePkg.dec
  MsCorePkg/MsCorePkg.dec
  ZeroTouchPkg/ZeroTouchPkg.dec

[LibraryClasses]
  BaseLib
  BaseMemoryLib
  DebugLib
  DevicePathLib
  DfciDeviceIdSupportLib
  DfciSettingsLib
  DfciUiSupportLib
  FileHandleLib
  HiiLib
  HttpLib
  MemoryAllocationLib
  JsonLiteParserLib
  PrintLib
  UefiBootServicesTableLib
  UefiDriverEntryPoint
  UefiHiiServicesLib
  UefiLib
  UefiRuntimeServicesTableLib
  ZeroTouchSettingsLib

[Guids]
  gDfciAuthProvisionVarNamespace
  gDfciConfigCompleteEventGroupGuid
  gDfciConfigStartEventGroupGuid
  gDfciMenuFormsetGuid
  gDfciRecoveryFormsetGuid
  gDfciPermissionManagerVarNamespace
  gDfciSettingsGuid
  gDfciSettingsManagerVarNamespace
  gEfiBootManagerPolicyNetworkGuid
  gEfiEventPreReadyToBootGuid

[Protocols]
  gDfciAuthenticationProtocolGuid
  gDfciSettingAccessProtocolGuid
  gDfciSettingPermissionsProtocolGuid
  gEfiBlockIoProtocolGuid
  gEfiBootManagerPolicyProtocolGuid
  gEfiHiiConfigAccessProtocolGuid
  gEfiHttpProtocolGuid
  gEfiHttpServiceBindingProtocolGuid
  gEfiSimpleFileSystemProtocolGuid
  gEfiUsbIoProtocolGuid

[FeaturePcd]

[Pcd]

[Depex]
  gEfiVariableArchProtocolGuid  AND
  gDfciSettingAccessProtocolGuid AND
  gDfciAuthenticationProtocolGuid AND
  gDfciSettingPermissionsProtocolGuid

[BuildOptions]
#  DEBUG_*_*_CC_FLAGS  = /FAcs

[BuildOptions.AARCH64]
  # LLVM doesn't support pragma warning push/pop or disabling specific warnings via pragmas
  # such pragmas found in included header DfciPacketHeader.h
  GCC:*_*_AARCH64_CC_FLAGS = -Wno-unknown-pragmas